{
  "name": "new-repo-manager",
  "version": "1.1.0",
  "description": "Simple projects software versions tracking",
  "author": "Nikita Glukhi <nikita.glukhi@valor-software.com>",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "build:watch": "ng build --watch",
    "build:server": "tsc -p apps/server/tsconfig.server.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ng serve",
    "start:server": "ts-node --compiler typescript --project apps/server/tsconfig.server.json apps/server/src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "lint": "ng lint",
    "test": "ng test"
  },
