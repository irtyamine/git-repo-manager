{
  "name": "GitHubControl",
  "version": "0.0.0",
  "description": "This application tracks versions of package.json files dependencies at github repositories",
  "author": "Nikita Glukhi <nikita.glukhi@valor-software.com>",
  "license": "MIT",
  "scripts": {
    "start:frontend": "cd src/static && ng serve",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:watcher": "ng build && nodemon --output-path dist --watch",
    "help": "ts-node --help",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "test:UI": "ng test --code-coverage",
    "test:server": "jest --detectOpenHandles",
    "test:server:cov": "jest --coverage",
    "getVersion": "npm view"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.10",
    "@angular/common": "6.1.10",
    "@angular/compiler": "6.1.10",
    "@angular/core": "6.1.10",
    "@angular/forms": "6.1.10",
    "@angular/http": "6.1.10",
    "@angular/platform-browser": "6.1.10",
    "@angular/platform-browser-dynamic": "6.1.10",
    "@angular/router": "6.1.10",
    "@nest-middlewares/cookie-session": "5.1.1",
    "@nestjs/common": "5.4.0",
    "@nestjs/core": "5.4.0",
    "@nestjs/mongoose": "5.2.2",
    "@nestjs/passport": "5.1.0",
    "@nestjs/websockets": "5.4.0",
    "@types/cron": "1.3.0",
    "@types/es6-promise": "3.3.0",
    "@types/lodash": "4.14.118",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cookie-session": "1.3.3",
    "core-js": "2.5.7",
    "cron": "1.6.0",
    "dotenv": "7.0.0",
    "lite-server": "2.4.0",
    "lodash": "4.17.19",
    "mongoose": "5.3.12",
    "ngx-bootstrap": "3.2.0",
    "node-cmd": "3.0.0",
    "nodemon": "1.18.10",
    "passport": "0.4.0",
    "passport-github2": "0.1.11",
    "passport-local": "1.0.0",
    "path": "0.12.7",
    "pm2": "3.5.1",
    "request": "2.88.0",
    "rxjs": "6.3.3",
    "semver": "5.6.0",
    "shelljs": "0.8.3",
    "short-uuid": "3.1.0",
    "to-array": "0.1.4",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.6",
    "@angular/cli": "6.2.4",
    "@angular/compiler-cli": "6.1.10",
    "@angular/language-service": "6.1.10",
    "@nestjs/testing": "5.4.0",
    "@types/express": "4.16.0",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/mongoose": "5.3.1",
    "@types/node": "10.12.9",
    "@types/supertest": "2.0.7",
    "codelyzer": "4.3.0",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "23.5.0",
    "karma": "3.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "prettier": "1.15.2",
    "supertest": "4.0.2",
    "ts-jest": "23.1.3",
    "ts-loader": "5.3.3",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.7.0",
    "tslint": "5.11.0",
    "typescript": "2.9.2",
    "typings-for-css-modules-loader": "1.7.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "src/repositories.controller",
      "src/authentication.controller",
      "src/api-tests"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage/back-end",
    "testEnvironment": "node"
  }
}
